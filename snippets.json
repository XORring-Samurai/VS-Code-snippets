	"Exponentiation": {
		"prefix": "Exp",
		"body": [
			"int Exp (int a, int b){",
			"        int res = 1;",
			"        assert(b >= 0);",
			"        while(b){",
			"                if(b & 1) res = (res * a);",
			"                a = (a * a);",
			"                b >>= 1;",
			"        }",
			"        return res;",
			"}"
		],
		"description": "Log output to console"
	},


