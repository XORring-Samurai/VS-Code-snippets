	"Nth_Root": {
		"prefix": "Nth-Root",
		"body": [
			"int Exp (int a, int b){",
			"        int res = 1;",
			"        assert(b >= 0);",
			"        while(b){",
			"                if(b & 1) res = (res * a);",
			"                a = (a * a);",
			"                b >>= 1;",
			"        }",
			"        return res;",
			"}",
			"",
			"int Nth_Root (int a, int n){",
			"        // adjust ub",
			"        int lb = 1, ub = 100001, ret = 1;",
			"        while(lb <= ub){",
			"                int mid = (lb + ub) >> 1;",
			"                int z = Exp(mid, n);",
			"                if(z <= a){",
			"                        ret = max(ret, mid);",
			"                        lb = mid + 1;",
			"                }",
			"                else ub = mid - 1;",
			"        }",
			"        return ret;",
			"}"
		],
		"description": "Log output to console"
	},

	"Matrix_Exponentiation": {
		"prefix": "MatExp",
		"body": [
			"vector <vector <int> > Multiply (vector <vector <int> > &a, vector <vector <int> > &b, int dim, int m){",
			"       vector <vector <int> > ret(dim, vector <int> (dim, 0));",
			"       for(int i = 0; i<dim; i++){",
			"               for(int j = 0; j<dim; j++){",
			"                       for(int k = 0; k<dim; k++){",
			"                               ret[i][k] += (a[i][j] * b[j][k]) % m;",
			"                               ret[i][k] %= mod;",
			"                       }",
			"               }",
			"       }",
			"       return ret;",
			"}",
			"", 
			"vector <vector <int> > MatExp (vector <vector <int> > &a, int dim, int n, int m){",
			"       vector <vector <int> > ret(dim, vector <int> (dim, 0));",
			"       for(int i = 0; i<dim; i++) ret[i][i] = 1;",
			"       assert(n >= 0);",
			"       while(n){",
			"               if(n & 1) ret = Multiply(ret, a, dim, m);",
			"               a = Multiply(a, a, dim, m);",
			"               n >>= 1;",
			"       }",
			"       return ret;",
			"}"
		],
		"description": "Log output to console"
	},

	"Fenwick_Tree": {
		"prefix": "Fenwick",
		"body": [
			"struct FenwickTree{",
			"        int s;",
			"        vector <int> bit;",
			"",
			"        void Initialize (int n){", 
			"                s = n + 1;",
			"                bit.assign(s, 0);", 
			"        }",
			"",
			"        void Update (int i, int x){",
			"                while(i < s){",
			"                        bit[i] += x;",
			"                        i = (i | (i + 1));",
			"                }",
			"        }",
			"",
			"        int Sum (int i){",
			"                int res = 0;",
			"                while(i >= 0){",
			"                        res += bit[i];",
			"                        i = (i & (i + 1)) - 1;",
			"                }",
			"                return res;",
			"        }", 
			"};"
		],
